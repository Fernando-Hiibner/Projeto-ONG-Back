    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Arquivo Atual",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Flask 3333",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_RUN_HOST": "0.0.0.0",
                "FLASK_RUN_PORT": "3333",
                "FLASK_ENV": "development",
                "FLASK_APP": "server.py",
                "FLASK_PORT": "3333"
            },
            "args": [
                "run",
            ],
            "jinja": true
        }
    ]
}